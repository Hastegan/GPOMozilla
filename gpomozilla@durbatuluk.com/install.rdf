<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	<Description about="urn:mozilla:install-manifest">

    <!-- Unique extension ID -->
		<em:id>gpomozilla@durbatuluk.com</em:id>

		<!-- 2 : extension -->
		<em:type>2</em:type>

    <!-- Extension name in Extensions Manager -->
		<em:name>GPO for Firefox, Thunderbird and Seamonkey</em:name>

    <!-- Extension version -->
		<em:version>0.1.3</em:version>

    <!-- Short description -->
    <em:localized>
       <Description>
           <em:locale>en-US</em:locale>
           <em:name>GPO support for Firefox, Thunderbird and Seamonkey</em:name>
           <em:description>Manage multiple Mozilla softwares preferences through GPO.</em:description>
       </Description>
   </em:localized>
   <em:localized>
       <Description>
           <em:locale>fr-FR</em:locale>
           <em:name>Support de GPO pour Firefox, Thunderbird et Seamonkey</em:name>
           <em:description>Gérer les préférences de plusieurs logiciels Mozilla par le biais de GPO.</em:description>
       </Description>
   </em:localized>

		<!-- Author -->
    <em:creator>Hastegan</em:creator>

    <!-- Extension website -->
		<em:homepageURL>http://www.gpomozilla.durbatuluk.com</em:homepageURL>

    <!-- Targeted apps -->
    <em:targetApplication>

			<!-- Thunderbird -->
			<Description>
				<em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
				<!-- Access to Windows Registry with XPCOM component was introduce in Gecko 1.8 (Firefox 1.5 / Thunderbird 1.5 / SeaMonkey 1.0) -->
				<em:minVersion>1.5</em:minVersion>
				<em:maxVersion>31.*</em:maxVersion>
			</Description>

      <!-- Firefox -->
      <Description>
        <!-- Access to Windows Registry with XPCOM component was introduce in Gecko 1.8 (Firefox 1.5 / Thunderbird 1.5 / SeaMonkey 1.0) -->
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>1.5</em:minVersion>
        <em:maxVersion>31.*</em:maxVersion>
      </Description>

       <!-- Seamonkey -->
      <Description>
        <!-- Access to Windows Registry with XPCOM component was introduce in Gecko 1.8 (Firefox 1.5 / Thunderbird 1.5 / SeaMonkey 1.0) -->
        <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
        <em:minVersion>1.0</em:minVersion>
        <em:maxVersion>2.*</em:maxVersion>
      </Description>

		</em:targetApplication>
	</Description>
</RDF>
